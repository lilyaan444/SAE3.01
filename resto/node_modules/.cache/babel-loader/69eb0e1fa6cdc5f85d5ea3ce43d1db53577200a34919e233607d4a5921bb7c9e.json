{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport router from '@/routers';\nimport axios from 'axios';\nexport default {\n  name: 'SignUp',\n  date() {\n    return {\n      email: '',\n      password: ''\n    };\n  },\n  methods: {\n    async signUp() {\n      let result = await axios.post('http://localhost:3000/user', {\n        email: this.email,\n        password: this.password\n      });\n      console.warn(result);\n      if (result.status === 201) {\n        localStorage.setItem('user-info', JSON.stringify(result.data));\n        this.$router.push({\n          name: 'HomePage'\n        });\n      }\n    }\n  },\n  mounted() {\n    let user = localStorage.getItem('user-info');\n    if (user) {\n      this.$router.push({\n        name: 'HomePage'\n      });\n    }\n  },\n  components: {\n    router\n  }\n};","map":{"version":3,"names":["router","axios","name","date","email","password","methods","signUp","result","post","console","warn","status","localStorage","setItem","JSON","stringify","data","$router","push","mounted","user","getItem","components"],"sources":["/Users/lilyaan/Desktop/resto/src/components/SignUp.vue"],"sourcesContent":["<template>\n  <img class=\"logo\" src=\"../assets/restologo.webp\" alt=\"Logo du resto\">\n<h1>Sign Up</h1>\n<div class=\"register\">\n  <label for=\"email\">Email</label>\n  <input type=\"email\" v-model=\"email\" id=\"email\" name=\"email\" />\n\n  <label for=\"password\">Password</label>\n  <input type=\"password\" v-model=\"password\" id=\"password\" name=\"password\" />\n\n  <button v-on:click=\"signUp\">Sign Up</button>\n  <p>\n    <router-link to=\"/login\">Login</router-link>\n  </p>\n</div>\n</template>\n\n<script>\nimport router from '@/routers';\nimport axios from 'axios';\nexport default {\n    name: 'SignUp',\n    date() {\n        return {\n            email: '',\n            password: '',\n        };\n    },\n    methods: {\n        async signUp() {\n            let result = await axios.post('http://localhost:3000/user', {\n                email: this.email,\n                password: this.password,\n            });\n            console.warn(result);\n            if (result.status === 201) {\n                localStorage.setItem('user-info', JSON.stringify(result.data));\n                this.$router.push({ name: 'HomePage' });\n            }\n        }\n    },\n    mounted() {\n        let user = localStorage.getItem('user-info');\n        if (user) {\n            this.$router.push({ name: 'HomePage' });\n        }\n    },\n    components: { router }\n}\n</script>\n\n<style>\n.logo{\n  width: 200px;\n  height: 160px;\n  margin: 0 auto;\n  display: block;\n}\n\n.register input{\n  width: 300px;\n  height: 30px;\n  padding-left: 20px;\n  display: block;\n  margin: 10px auto;\n  margin-bottom: 10px;\n  border-radius: 5px;\n  border: 1px solid orange;\n}\n\n.register button{\n  width: 300px;\n  height: 30px;\n  border-radius: 5px;\n  border: 1px solid orange;\n  background-color: orange;\n  cursor: pointer;\n}\n</style>"],"mappings":";AAkBA,OAAOA,MAAK,MAAO,WAAW;AAC9B,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAe;EACXC,IAAI,EAAE,QAAQ;EACdC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE;IACd,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACL,MAAMC,MAAMA,CAAA,EAAG;MACX,IAAIC,MAAK,GAAI,MAAMP,KAAK,CAACQ,IAAI,CAAC,4BAA4B,EAAE;QACxDL,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,QAAQ,EAAE,IAAI,CAACA;MACnB,CAAC,CAAC;MACFK,OAAO,CAACC,IAAI,CAACH,MAAM,CAAC;MACpB,IAAIA,MAAM,CAACI,MAAK,KAAM,GAAG,EAAE;QACvBC,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEC,IAAI,CAACC,SAAS,CAACR,MAAM,CAACS,IAAI,CAAC,CAAC;QAC9D,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;UAAEjB,IAAI,EAAE;QAAW,CAAC,CAAC;MAC3C;IACJ;EACJ,CAAC;EACDkB,OAAOA,CAAA,EAAG;IACN,IAAIC,IAAG,GAAIR,YAAY,CAACS,OAAO,CAAC,WAAW,CAAC;IAC5C,IAAID,IAAI,EAAE;MACN,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC;QAAEjB,IAAI,EAAE;MAAW,CAAC,CAAC;IAC3C;EACJ,CAAC;EACDqB,UAAU,EAAE;IAAEvB;EAAO;AACzB"},"metadata":{},"sourceType":"module","externalDependencies":[]}